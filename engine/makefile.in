INCLUDES=%INCLUDES%
LIBS=%LIBS%
CC=%CC%
CFLAGS=%CFLAGS%
INCLUDES=%INCLUDES%
DEFINES=%DEFINES%
LD=%LD%
LDFLAGS=%LDFLAGS%
LDEXTRA=%LDEXTRA%
LDSO=%LDSO%
OBJECT=e_gem.o

default: all

all:
	$(CC) $(CFLAGS) $(INCLUDES) $(DEFINES) -c -g -o $(OBJECT) e_gem.c
	$(LD) $(LDFLAGS) -o $(LDSO) $(OBJECT) $(LDEXTRA) $(LIBS) -lcrypto
	file $(LDSO)

clean:
	rm -f $(OBJECT)
	rm -f $(LDSO)

.PHONY: default all clean

